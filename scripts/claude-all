                    echo -e "${GREEN}‚úì Agent created successfully!${NC}"
                    echo -e "${GREEN}Agent ID: $agent_id${NC}"
                else
                    echo -e "${RED}‚ùå Failed to create agent${NC}"
                    echo "Response: $response"
                    exit 1
                fi
                ;;
